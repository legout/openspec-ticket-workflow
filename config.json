{
  "templateRepo": "legout/openspec-ticket-opencode-starter",
  "templateRef": "v0.6.0",
  "useWorktrees": true,
  "worktreeDir": ".worktrees",
  "defaultParallel": 3,
  "mainBranch": "main",
  "autoPush": true,
  "unsafe": {
    "allowParallel": true,
    "allowDirtyDone": true,
    "commitStrategy": "prompt"
  },
  "planner": {
    "model": "openai/gpt-5.2",
    "reasoningEffort": "high",
    "temperature": 0.5
  },
  "worker": {
    "model": "zai-coding-plan/glm-4.7",
    "fallbackModels": [
      "minimax/MiniMax-M2.1"
    ],
    "reasoningEffort": "none",
    "temperature": 0.75
  },
  "reviewer": {
    "autoTrigger": false,
    "policy": "gate",
    "blockSeverities": [
      "error"
    ],
    "blockMinConfidence": 80,
    "followupSeverities": [
      "warning"
    ],
    "followupMinConfidence": 60,
    "skipTags": [
      "no-review",
      "wip"
    ],
    "autoRerunOnDone": false,
    "roles": {
      "bug-footgun": {
        "enabled": true,
        "model": "openai/gpt-5.2-codex",
        "reasoningEffort": "high",
        "temperature": 0
      },
      "spec-audit": {
        "enabled": true,
        "model": "openai/gpt-5.2-codex",
        "reasoningEffort": "high",
        "temperature": 0
      },
      "generalist": {
        "enabled": true,
        "model": "openai/gpt-5.2",
        "reasoningEffort": "medium",
        "temperature": 0
      },
      "second-opinion": {
        "enabled": true,
        "model": "anthropic/opus-4.5",
        "reasoningEffort": "max",
        "temperature": 0
      }
    }
  },
  "agents": "pi"
}
